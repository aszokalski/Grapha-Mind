    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aszokalski/Grapha-Mind"
  },
  "keywords": [
    "1Mind"
  ],
  "bugs": {
    "url": "https://github.com/aszokalski/Grapha-Mind/issues"
