{
  "name": "one-mind-beta",
  "version": "0.0.1",
  "description": "Mind map for teams",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64",
    "build:linux": "electron-builder build --deb --x64"
  },
  "build": {
    "appId": "com.1mind.beta",
    "productName": "1Mind",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "0.0.1"
  },
  "author": "1Mind",
  "license": "MIT",
  "dependencies": {
    "@lottiefiles/react-lottie-player": "^3.4.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/opentype": "0.0.4",
    "@types/opentype.js": "^1.3.1",
    "@types/react": "^16.9.13",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.4",
    "arraybuffer-loader": "^1.0.8",
    "copy-pkg-json-webpack-plugin": "0.0.40",
    "electron-util": "^0.14.2",
    "fontkit": "^1.8.1",
    "get-system-fonts": "^2.0.2",
    "gojs": "^2.1.50",
    "gojs-react": "^1.0.10",
    "immer": "^8.0.0",
    "jquery": "^3.6.0",
    "jspdf": "^2.4.0",
    "local-storage": "^2.0.0",
    "mongodb": "^3.7.1",
    "mongodb-client-encryption": "^1.2.7",
    "mui-layout": "^1.2.9",
    "opentype": "^0.1.2",
    "opentype.js": "^1.3.3",
    "pdfkit": "^0.12.1",
    "peerjs": "^1.3.2",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-id-generator": "^3.0.2",
    "react-uuid": "^1.0.2",
    "saslprep": "^1.0.3",
    "string-replace-webpack-plugin": "^0.1.3",
    "svg-to-pdfkit": "^0.1.8",
    "svg2pdf.js": "^2.2.0",
    "transform-loader": "^0.2.4"
  },
  "devDependencies": {
    "@types/fontkit": "^1.8.0",
    "@types/jquery": "^3.5.5",
    "@types/mocha": "^5.2.7",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.5",
    "acorn": "^6.4.0",
    "ajv": "^6.10.2",
    "axios": "^0.21.1",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "electron": "^3.1.13",
    "electron-builder": "^20.44.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.26.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "js-loader": "^0.1.1",
